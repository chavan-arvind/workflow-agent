{
    "name": "gitclone-and-analyze-cloud-function",
    "version": "1.0.0",
    "description": "Cloud Functions to clone a Git repository, store it in Cloud Storage, and analyze code using Gemini API",
    "main": "index.js",
    "scripts": {
        "start": "functions-framework --target=cloneRepoToStorage,analyzeCode",
        "deploy-clone": "gcloud functions deploy cloneRepoToStorage --runtime nodejs16 --trigger-http --allow-unauthenticated",
        "deploy-analyze": "gcloud functions deploy analyzeCode --runtime nodejs16 --trigger-http --allow-unauthenticated"
    },
    "dependencies": {
        "@google-cloud/functions-framework": "^3.2.0",
        "@google-cloud/storage": "^6.11.0",
        "@google/generative-ai": "^0.1.3",
        "tmp": "^0.2.1"
    },
    "devDependencies": {
        "@google-cloud/functions-framework": "^3.2.0"
    },
    "engines": {
        "node": ">=16.0.0"
    }
}